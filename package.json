{
  "name": "json-data-file",
  "version": "0.10.4",
  "description": "Ler e Gravar dados em um arquivo JSON.",
  "main": "index.js",
  "config": {
    "commitMessage": "Atualização de segurança"
  },
  "scripts": {
    "dev": "node index.js",
    "git": "npm run gst && npm run gad && npm run gco && npm run gta && npm run gpo && npm run gpg",
    "gst": "git status",
    "gad": "git add .",
    "gco": "cross-var git commit -m \"$npm_package_config_commitMessage\"",
    "gta": "cross-var git tag v$npm_package_version",
    "gpo": "git push origin master --tags",
    "gpg": "git push gitlab master --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digitalbocca/json-data-file.git"
  },
  "keywords": [
    "node",
    "json",
    "dados",
    "gravar",
    "ler",
    "arquivo"
  ],
  "author": "Gabriel Bertola Bocca - gabriel at estudiodigitalbocca.com.br",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/digitalbocca/json-data-file/issues"
  },
  "homepage": "https://github.com/digitalbocca/json-data-file#readme",
  "devDependencies": {
    "cross-var-no-babel": "^1.2.0",
    "standard": "^14.3.4"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "json-format": "^1.0.1",
    "moment": "^2.27.0"
  }
}
